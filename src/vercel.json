{
    "version": 2,
    "name": "petgram-django-crud-cliente",
    
    "builds": [
        { "use": "@vercel/wsgi.py", 
           "src": "django-crud-cliente/seguros_cliente_crud_api/wsgi.py",
           "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}}
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "django-crud-cliente/seguros_cliente_crud_api/wsgi.py"            
        }
      
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/" }]
} 
